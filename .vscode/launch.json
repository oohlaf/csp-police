    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch webd file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/cmd/webd/main.go",
            "env": {
                "DEBUG": "true",
                "WEB_PORT": "8080"
            }
        },
        {
            "name": "Launch grpcd file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/cmd/grpcd/main.go",
            "env": {
                "DEBUG": "true",
                "GRPC_PORT": "9090"
            }
        },
        {
            "name": "Launch migrate file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/cmd/migrate/main.go",
            "env": {
                "DEBUG": "true",
            }
        }
    ]
}